<submit-form-after-reauthentication>
  <%= form_wrapper do |f| %>
    <%= f.hidden_field :reauthentication_token, id: dom_id(passkey, :reauthentication_token) %>
    <%= f.button_component(type: :submit, color_variant: :danger) do %>
      <%= render helpers.icon_set.passkey_icon %>
      <%= t("user_settings.passkeys.destroy.form.submit_button_title") %>
    <% end %>
  <% end %>
</submit-form-after-reauthentication>
